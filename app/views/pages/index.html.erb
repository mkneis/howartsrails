<!-- <h1 class="mb-4 text-white">Personajes</h1> -->


<table class="table table-dark table-bordered table-hover">

    <thead>
      <tr>
        <th scope="col">id</th>
        <th scope="col">Name</th>
        <th scope="col">Location</th>
        <th scope="col">House</th>
      </tr>
    </thead>
  
    <tbody>
      <% @personajes.each do |p| %>
          <tr>
              <th scope="row"><%= p.id%></th>
              <td><%= p.name%></td>
              <td><%= p.location%></td>
              <td><%= p.house%></td>
          </tr>
      <% end %>
    </tbody>
</table> 